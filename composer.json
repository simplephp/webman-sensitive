{
    "name": "simplephp/webman-sensitive",
    "description": "Webman 敏感词检测，过滤，标记",
    "license": "MIT",
    "authors": [
        {
            "name": "simplephp",
            "email": "841694874@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "workerman/webman-framework": ">=1.4",
        "ext-mbstring": "*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "simplephp\\sensitive\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "dev-dev"
        }
    }
}
